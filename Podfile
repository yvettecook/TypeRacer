# Uncomment this line to define a global platform for your project
platform :ios, '9.0'

target 'TypeRacer' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for TypeRacer
  pod 'RxSwift', '~> 2.5.0'
  pod 'RxCocoa', '~> 2.5.0'

  target 'TypeRacerTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxSwift', '~> 2.5.0'
    pod 'RxTests', '~> 2.5.0'
  end

  target 'TypeRacerUITests' do
    inherit! :search_paths

  end

end
